@model WebDT.Models.BoNho

@{
    ViewData["Title"] = "Delete";
    Layout = "_DashBoardAdmin";
}

<h1>Xóa </h1>

<h3>Bạn có chắc chắn muốn xóa?</h3>
<div>
    <h4>Bộ nhớ</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Dung lượng bộ nhớ 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DungLuongBoNho)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="MaBoNho" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Trở về</a>
    </form>
</div>
