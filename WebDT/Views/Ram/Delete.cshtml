@model WebDT.Models.Ram

@{
    ViewData["Title"] = "Delete";
}

<h1>Xóa</h1>

<h3>Bạn có chắc chắn muốn xóa?</h3>
<div>
    <h4>Ram</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Tên ram 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TenRam)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="MaRam" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Trở về</a>
    </form>
</div>
